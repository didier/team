<!DOCTYPE html>
<html lang="en">

  <%- include ("./partials/head") %>

  <body>

    <main>
      <h1>Hello <%= user.firstName %> <%= user.lastName %> </h1>
      <ul>
        <% users.forEach(user => { %>
        <li><%- user.name %></li>
        <% }) %>
      </ul>


      <form action="/logout" method="POST">
        <button type="submit">Logout</button>
      </form>

    </main>
		<%- include ('./partials/filter') %>
		<%- include ('./partials/match', { users }) %>
  </body>
</html>

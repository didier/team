<!DOCTYPE html>
<html lang="en">
  <%- include ("./partials/head") %>

  <body>
    <main>
      <h1>Hello world</h1>
      <ul>
        <% users.forEach(user => { %>
        <li><%- user.firstName %> <a href="/chat/<%- user._id %>">Chat</a></li>
        <% }) %>
      </ul>

      <form action="/logout" method="POST">
        <button type="submit">Logout</button>
      </form>

      <a href="/chat"></a>
    </main>
    <%- include ("./partials/filter") %>
  </body>
</html>
